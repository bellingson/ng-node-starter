<div class="container">

    <h1>Bookings</h1>

    <table class="table table-striped table-hover">
        <thead>
        <tr>
            <td></td>
            <th>Booking</th>
            <th>Start</th>
            <th>End</th>
            <th>Days</th>

        </tr>
        </thead>
        <tbody>

        {{#each events}}
        <tr>
            <td>{{@index}}</td>
            <td>
                {{summary}}
            </td>
            <td>{{date start}}</td>
            <td>{{date end}}</td>
            <td>{{days}}</td>
        </tr>
        {{/each}}

        </tbody>
    </table>
    
</div>

